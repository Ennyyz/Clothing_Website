}

<h2>CHI TIẾT</h2>

<div>
    @*<h4>OrderPro</h4>*@
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Ngày giao hàng :
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DateOrder)
        </dd>

        <dt>
            Địa chỉ giao hàng :
        </dt>

        <dd>
            @Html.DisplayFor(model => model.AddressDeliverry)
        </dd>

        <dt>
